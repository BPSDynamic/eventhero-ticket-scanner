# AWS Amplify Configuration
# Copy this file to .env.local and fill in your actual values

# AWS Region where your Cognito User Pool is located
NEXT_PUBLIC_AWS_REGION=us-east-1

# Your Cognito User Pool ID (format: us-east-1_xxxxxxxxx)
NEXT_PUBLIC_USER_POOL_ID=your-user-pool-id

# Your Cognito User Pool Client ID
NEXT_PUBLIC_USER_POOL_CLIENT_ID=your-user-pool-client-id

# GraphQL API Configuration
# Your AppSync GraphQL API URL
NEXT_PUBLIC_TICKET_APPSYNC_API_URL=https://your-api-id.appsync-api.your-region.amazonaws.com/graphql

# Your AppSync API Key
NEXT_PUBLIC_TICKET_APPSYNC_API_KEY=your-appsync-api-key
